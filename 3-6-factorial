class Factorial {
 public static void main(String[] args) {
 Scanner scanner = new Scanner(System.in);
 System.out.print("Enter the value of a: ");
 int a = scanner.nextInt();
 if (a < 0) {
 System.out.println("No factorial");
 } else {
 int x = 1;
 for (int i = 1; i <= a; i++) {
 x *= i;
 }
 System.out.println(x);
 }
 }
}
